{
  "expo": {
    "name": "SaxionAAD-Filametric",
    "slug": "SaxionAAD-Filametric",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "saxionaadfilametric",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Filametric_Full_Logo_v3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bassaxion.SaxionAADFilametric",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs permissions to open the camera for adding dryers using QR-Codes"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bassaxion.SaxionAADFilametric",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Deze app heeft camera toegang nodig om barcodes en QR-codes te scannen."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f2158be3-3d16-490d-9f28-27232fbf3625"
      }
    },
    "owner": "bassaxion"
  }
}
